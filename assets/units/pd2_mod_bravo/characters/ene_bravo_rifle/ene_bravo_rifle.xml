			<object enabled="true" name="'g_body_black_sg'"/>
			<object enabled="true" name="'g_body_black_sg_lod1'"/>
		</sequence>
		<sequence editable_state="true" name="'body_black_default'" once="true" triggable="true">
			<object enabled="true" name="'g_body_black'"/>
			<object enabled="true" name="'g_body_black_lod1'"/>
			
			<object enabled="false" name="'g_arms_a'"/>
			<object enabled="false" name="'g_arms_b'"/>
			<object enabled="false" name="'g_arms_c'"/>
			<object enabled="false" name="'g_arms_d'"/>
