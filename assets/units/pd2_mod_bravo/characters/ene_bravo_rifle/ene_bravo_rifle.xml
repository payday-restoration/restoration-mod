		<sequence editable_state="true" name="'body_sleeveup'" once="true" triggable="true">
			<object enabled="true" name="'g_body_sg'"/>
			<object enabled="true" name="'g_body_sg_lod1'"/>
		</sequence>
		
		<sequence editable_state="true" name="'body_lmg'" once="true" triggable="true">
			<object enabled="true" name="'g_body_lmg'"/>
			<object enabled="true" name="'g_body_lmg_lod1'"/>
			<object enabled="true" name="'g_swat_lmg'"/>
			<object enabled="true" name="'g_swat_lmg_lod1'"/>
		</sequence>
		
		<!-- RANDOM HOLSTERS end -->

		<!-- BALACLAVA W/ "INSIDE" FACE SEQUENCES -->
		<sequence editable_state="true" name="'head_balaclava'" triggable="true" once="true">	
