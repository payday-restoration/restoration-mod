<sequence_manager>
	<unit>
		<variables>
										
			<!-- heavy_fbi swat -->
			<var_gadgets_city_heavy value="0"/>	
			
			<var_gadgets_flashlight value="0"/>					
			
			<!-- fbi swat -->
			<var_gadgets_city_light value="0"/>					
			
			<!-- goggle a -->
			<var_goggles value="0"/>					
		</variables>
	

		<body name="'glass_swat'">
			<endurance explosion="4" bullet="1">
				<run_sequence name="'crk_goggles'"/>
			</endurance>
		</body>	
				
		<body name="'visor'">
			<endurance explosion="4" bullet="1">
				<run_sequence name="'crk_visor'"/>
			</endurance>	
			<endurance explosion="6" >
				<run_sequence name="'destroy_altyn'"/>
			</endurance>
		</body>	
		
		<body name="'glass_c'">
			<endurance bullet="1">
				<run_sequence name="'crk_goggles'"/>
			</endurance>
		</body>	

		<filter name="'check_city_swat_ar'">
			<check value="vars.var_gadgets_city_light == 1"/>
		</filter>		
		<filter name="'check_city_swat_sg'">
			<check value="vars.var_gadgets_city_light == 2"/>
		</filter>
		<filter name="'check_city_swat_smg'">
			<check value="vars.var_gadgets_city_light == 3"/>
		</filter>
		<filter name="'check_city_shield'">
			<check value="vars.var_gadgets_city_light == 4"/>
		</filter>
		<filter name="'check_city_sniper'">
			<check value="vars.var_gadgets_city_light == 5"/>
		</filter>

		<filter name="'check_goggle_a'">
			<check value="vars.var_goggles == 1"/>
		</filter>

		<filter name="'check_flashlights_on_sg'">
			<check value="vars.var_gadgets_flashlight == 1"/>
			<check value="vars.var_gadgets_city_light == 2"/>
		</filter>

		<filter name="'check_flashlights_on_ar'">
			<check value="vars.var_gadgets_flashlight == 1"/>
			<check value="vars.var_gadgets_city_light == 1"/>
		</filter>

		<filter name="'check_goggle_b'">
			<check value="vars.var_goggles == 2"/>
		</filter>
		
		<filter name="'check_goggle_c'">
			<check value="vars.var_goggles == 3"/>
		</filter>
		
		<filter name="'check_goggle_d'">
			<check value="vars.var_goggles == 4"/>
		</filter>
		
		
		<sequence editable_state="true" name="'swat_ar'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="1"/>			
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_ar_helmetlight'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="1"/>
			<set_variables var_gadgets_flashlight="1"/>			
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_sg'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="2"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'swat_sg_nvg'" triggable="true" once="true">	
			<set_variables var_gadgets_city_light="2"/>		
			<set_variables var_gadgets_flashlight="1"/>
			<run_sequence name="'gadget_meat_grinder'"/>					
		</sequence>

		<sequence editable_state="true" name="'swat_smg'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="3"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_shield'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="4"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'swat_sniper'" triggable="true" once="true">						
			<set_variables var_gadgets_city_light="5"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'gadget_meat_grinder'" triggable="true" once="true">	
			
			<!-- rifleman -->
			<run_sequence filter="'check_city_swat_ar'" name="'head_'..pick('balaclava_b', 'balaclava_b', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'a','b','c', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava',  'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava')"/>
			<object filter="'check_city_swat_ar'" enabled="true" name="'g_swat_ar'"/>						
			<object filter="'check_city_swat_ar'" enabled="true" name="'g_swat_ar_lod1'"/>
			<run_sequence filter="'check_city_swat_ar'" name="'holster_'..pick('r','r','r','r','l','l','twinnem')"/>
			<run_sequence filter="'check_city_swat_ar'" name="'helmet'..pick('_flashlight','_flashlight','_flashlight','_flashlight','_flashlight','nun_actually','_flashlight')"/>
			
			<!-- extra attachments! :D -->
			<run_sequence filter="'check_city_swat_ar'" name="'helmet'..pick('_chops','_chops','_nun','_headset','_chops','_nun','_nun')"/>

			<!-- ar (flashlight on)-->
			<run_sequence filter="'check_flashlights_on_ar'" name="'flashlight_on'"/>
			<run_sequence filter="'check_flashlights_on_ar'" name="'helmet_flashlight'"/>
		
			<!-- sg -->
			<run_sequence filter="'check_city_swat_sg'" name="'head_'..pick('balaclava_b',  'balaclava_b', 'balaclava',  'balaclava',  'balaclava', 'balaclava_b', 'balaclava', 'balaclava', 'balaclava', 'a','b','c', 'balaclava', 'balaclava_b','balaclava_b','balaclava_b','balaclava_b','balaclava_b','balaclava_b','balaclava_b','balaclava')"/>
			
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_swat_sg'"/>						
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_swat_sg_lod1'"/>
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_body_shitbag'"/>						
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_body_shitbag_lod1'"/>
			<object filter="'check_city_swat_sg'" enabled="false" name="'g_body'"/>						
			<object filter="'check_city_swat_sg'" enabled="false" name="'g_body_lod1'"/>
			
			<run_sequence filter="'check_city_swat_sg'" name="'holster_'..pick('r','r','r','r','l','l','twinnem')"/>

			<!-- extra attachments! :D -->
			<run_sequence filter="'check_city_swat_sg'" name="'helmet'..pick('_mask','_mask','_mask','_mask','_mask','_mask','_mask','_mask','_chops','_chops', '_mask')"/>
			
			<run_sequence filter="'check_city_swat_sg'" name="'helmet_nvg_up'"/>
		
			<!-- sg (nvgs on)-->
			<run_sequence filter="'check_flashlights_on_sg'" name="'helmet_nvg_down'"/>

			<!-- smg -->			
			<run_sequence filter="'check_city_swat_smg'" name="'head_'..pick('balaclava', 'balaclava', 'balaclava', 'balaclava_b', 'balaclava_b', 'balaclava', 'balaclava', 'a','b','c', 'balaclava', 'balaclava')"/>
						
			<object filter="'check_city_swat_smg'" enabled="true" name="'g_swat_smg'"/>						
			<object filter="'check_city_swat_smg'" enabled="true" name="'g_swat_smg_lod1'"/>
			<object filter="'check_city_swat_smg'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_city_swat_smg'" enabled="false" name="'g_helmet_lod2'"/>
			<object filter="'check_city_swat_smg'" enabled="true" name="'g_hat_lod1'"/>
			<object filter="'check_city_swat_smg'" enabled="true" name="'g_hat_lod2'"/>			
			<decal_mesh filter="'check_city_swat_smg'" enabled="false" name="'dm_helmet'"/>					

			<run_sequence filter="'check_city_swat_smg'" name="'totally_'..pick('nothing', 'nothing', 'random', 'nothing')"/>
			
			<run_sequence filter="'check_city_swat_smg'" name="'holster_'..pick('r','r','r','r','l','l','twinnem')"/>

			<run_sequence filter="'check_city_swat_smg'" name="'helmet'..pick('_headset_ng', '_headset_ng','_headset_ng','_headset_smg', '_headset_smg', '_nun_smg')"/>

			<!-- shield -->			
			<object filter="'check_city_shield'" enabled="true" name="'g_shield'"/>						
			<object filter="'check_city_shield'" enabled="true" name="'g_shield_lod1'"/>
			<object filter="'check_city_shield'" enabled="true" name="'g_body_shitbag'"/>						
			<object filter="'check_city_shield'" enabled="true" name="'g_body_shitbag_lod1'"/>
			<object filter="'check_city_shield'" enabled="false" name="'g_body'"/>						
			<object filter="'check_city_shield'" enabled="false" name="'g_body_lod1'"/>			
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod2'"/>
			
			<run_sequence filter="'check_city_shield'" name="'head_'..pick('balaclava', 'balaclava', 'balaclava', 'balaclava_b', 'balaclava_b', 'balaclava', 'balaclava', 'a','b','c', 'balaclava', 'balaclava')"/>
			
			<run_sequence filter="'check_city_shield'" name="'helmet_shield'"/>
			
			<run_sequence filter="'check_city_shield'" name="'holster_'..pick('r_shield','l_shield')"/>
			
						
			<!-- sniper -->			
			<object filter="'check_city_sniper'" enabled="true" name="'g_sniper'"/>						
			<object filter="'check_city_sniper'" enabled="true" name="'g_sniper_lod1'"/>
			<object filter="'check_city_sniper'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_city_sniper'" enabled="false" name="'g_helmet_lod2'"/>
			<object filter="'check_city_sniper'" enabled="true" name="'g_hat_lod1'"/>
			<object filter="'check_city_sniper'" enabled="true" name="'g_hat_lod2'"/>					
			<decal_mesh filter="'check_city_sniper'" enabled="false" name="'dm_helmet'"/>					
			<run_sequence filter="'check_city_sniper'" name="'head_'..pick('a','b','c', 'balaclava', 'balaclava_b')"/>
			
		</sequence>

		<sequence editable_state="true" name="'crk_visor'" once="true" triggable="true">
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod22'"/>
			<object filter="'check_city_shield'" enabled="true" name="'g_helmet_lod23'"/>
			<effect filter="'check_city_shield'" damp_time="5" name="'effects/particles/bullet_hit/glass_breakable/bullet_hit_glass_breakable'" parent="object( 'Head' )" position="v()"/>
		</sequence>				

		<sequence editable_state="true" name="'destroy_altyn'" once="true" triggable="true">
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod22'"/>
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod23'"/>
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod20'"/>
			<object filter="'check_city_shield'" enabled="false" name="'g_helmet_lod21'"/>
			
			<body 			filter="'check_city_shield'" enabled="false" name="'visor'" />
			<decal_mesh 	filter="'check_city_shield'" enabled="false" name="'c_shield_glass'" />

			<body filter="'check_city_shield'" enabled="false" name="'shield_mask'" />	
			<decal_mesh filter="'check_city_shield'" enabled="false" name="'c_visor'" />													
			
			<effect filter="'check_city_shield'" damp_time="4" name="'effects/payday2/particles/impacts/metal_impact_pd2'" parent="object( 'Head' )" position="v()"/>			
			<effect filter="'check_city_shield'" damp_time="5" name="'effects/payday2/particles/impacts/metal_impact_pd2'" parent="object( 'Head' )" position="v()"/>
			
			<sound action="'play'" event="'swatturret_weakspot_hit'" object="'Head'"/>					
		</sequence>				


		<sequence editable_state="true" name="'totally_random'" once="true" triggable="true">
			<object enabled="true" name="'g_body_shitbag'"/>
			<object enabled="true" name="'g_body_shitbag_lod1'"/>
			<object enabled="false" name="'g_body'"/>
			<object enabled="false" name="'g_body_lod1'"/>
		</sequence>

		<sequence editable_state="true" name="'goggles_a'" once="true" triggable="true">
			<object enabled="true" name="'g_goggles_a'"/>
			<object enabled="true" name="'g_goggles_a_lod1'"/>
			<object enabled="true" name="'g_glass'"/>
			<set_variables var_goggles="1"/>					
			<decal_mesh enabled="true" name="'c_glass'" />					
			<body enabled="true" name="'glass_swat'" />										
		</sequence>
		
		<sequence editable_state="true" name="'goggles_b'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod3'"/>
			<object enabled="true" name="'g_helmet_lod4'"/>
			<object enabled="true" name="'g_glass'"/>
			
			<set_variables var_goggles="2"/>										
			<decal_mesh enabled="true" name="'c_glass'" />					
			<body enabled="true" name="'glass_swat'" />										
		</sequence>
		
		<sequence editable_state="true" name="'goggles_c'" once="true" triggable="true">
			<object enabled="true" name="'g_goggles_c'"/>
			<object enabled="true" name="'g_goggles_c_lod1'"/>
			<object enabled="true" name="'g_glass_c'"/>
			<set_variables var_goggles="3"/>										
			<decal_mesh enabled="true" name="'c_glass_c'" />					
			<body enabled="true" name="'glass_c'" />										
		</sequence>
		
		<sequence editable_state="true" name="'goggles_e'" once="true" triggable="true">
			<object enabled="true" name="'g_glasses'"/>
			<object enabled="true" name="'g_glasses_glass'"/>
			<set_variables var_goggles="5"/>										
		</sequence>
		
		<sequence editable_state="true" name="'crk_goggles'" once="true" triggable="true">
			<object filter="'check_goggle_a'" enabled="true" name="'g_glass_crk'"/>
			<object filter="'check_goggle_a'" enabled="false" name="'g_glass'"/>
			
			<object filter="'check_goggle_b'" enabled="true" name="'g_glass_crk'"/>
			<object filter="'check_goggle_b'" enabled="false" name="'g_glass'"/>
			
			<object filter="'check_goggle_c'" enabled="false" name="'g_glass_c'"/>
			<object filter="'check_goggle_c'" enabled="true" name="'g_glass_c_crk'"/>		
			
			<object filter="'check_goggle_d'" enabled="false" name="'g_glass_d'"/>
			<object filter="'check_goggle_d'" enabled="true" name="'g_glass_d_crk'"/>		
		</sequence>				
		
		<sequence editable_state="true" name="'helmet_headset'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a',a',a',a',a',a',a',a',a',a',a',a','a','c','a','b','b','e',a',a',a',a',a',a','a','a')"/>								
		
			<object enabled="true" name="'g_headset'"/>
			<object enabled="true" name="'g_headset_lod1'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_headset_smg'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a','a','a','a','a','a','a','a','a','a','a','a','a','a','a','c','a','e','a','a')"/>								
		
			<object enabled="true" name="'g_headset'"/>
			<object enabled="true" name="'g_headset_lod1'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_headset_ng'" once="true" triggable="true">		
			<object enabled="true" name="'g_headset'"/>
			<object enabled="true" name="'g_headset_lod1'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_nun'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a',a',a',a',a',a',a',a',a',a',a',a','a','c','a','b','b','e',a',a',a',a',a',a','a','a')"/>								
		</sequence>

		<sequence editable_state="true" name="'helmet_nun_smg'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a','a','a','a','a','a','a','a','a','a','a','a','a','a','a','c','a','e','a','a')"/>								
		</sequence>
		
		<sequence editable_state="true" name="'helmet_chops'" once="true" triggable="true">
			<run_sequence name="'goggles_'..pick('a','a',a',a',a',a',a',a',a',a',a',a',a','a','c','a','b','b','e',a',a',a',a',a',a','a','a')"/>								
		
			<object enabled="true" name="'g_helmet_lod11'"/>
			<object enabled="true" name="'g_helmet_lod12'"/>

			<body enabled="true" name="'shield_mask'" />								
			<decal_mesh enabled="true" name="'c_visor'" />								
		</sequence>

		<sequence editable_state="true" name="'helmet_flashlight'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod7'"/>
			<object enabled="true" name="'g_helmet_lod8'"/>
		</sequence>

		<sequence editable_state="true" name="'flashlight_on'" once="true" triggable="true">
			<effect_spawner enabled="true" name="'es_flashlight'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_nvg_up'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod9'"/>
			<object enabled="true" name="'g_helmet_lod10'"/>
		</sequence>

		<sequence editable_state="true" name="'helmet_shield'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod13'"/>
			<object enabled="true" name="'g_helmet_lod14'"/>			
			
			<run_sequence name="'helmet_'..pick('shield_visor','shield_visor','shield_visor','shield_goggles','shield_visor','shield_visor','shield_visor')"/>			
		</sequence>

		<sequence editable_state="true" name="'helmet_shield_visor'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod20'"/>						
			<object enabled="true" name="'g_helmet_lod21'"/>
			<object enabled="true" name="'g_helmet_lod22'"/>

			
			<body enabled="true" name="'visor'" />
			<decal_mesh enabled="true" name="'c_shield_glass'" />	
			
			
			<body enabled="true" name="'shield_mask'" />	
			<decal_mesh enabled="true" name="'c_visor'" />

		</sequence>		
		
		<sequence editable_state="true" name="'helmet_shield_goggles'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod20'"/>						
			<object enabled="true" name="'g_helmet_lod21'"/>
			
			
			<body enabled="true" name="'shield_mask'" />	
			<decal_mesh enabled="true" name="'c_visor'" />
			<run_sequence name="'mask_goggles'"/>						
		</sequence>		
		
		<sequence editable_state="true" name="'helmet_mask'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod5'"/>
			<object enabled="true" name="'g_helmet_lod6'"/>

			<object enabled="false" name="'g_helmet_lod1'"/>						
			<object enabled="false" name="'g_helmet_lod2'"/>
			
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_helmet_lod13'"/>						
			<object filter="'check_city_swat_sg'" enabled="true" name="'g_helmet_lod14'"/>
			
			<run_sequence name="'helmet_'..pick('actnun','actnun','actnun','actnun','actnun','actnun','actnun','headset_ng','actnun','actnun')"/>			
			
			<run_sequence name="'mask_'..pick('glass','glass','glass','glass','glass','glass','glass','glass','glass','glass','glass','goggles','glass','glass','glass')"/>			
		</sequence>

		<sequence editable_state="true" name="'mask_glass'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod17'"/>
		</sequence>

		<sequence editable_state="true" name="'mask_goggles'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod18'"/>
			<object enabled="true" name="'g_helmet_lod19'"/>
			<object enabled="true" name="'g_glass_d'"/>

			<set_variables var_goggles="4"/>					
			
			<decal_mesh filter="'check_goggle_d'" enabled="true" name="'c_glass'" />										
			<body filter="'check_goggle_d'" enabled="true" name="'glass_swat'" />													
		</sequence>

		<sequence editable_state="true" name="'shield_visor'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod22'"/>
			<object enabled="false" name="'g_helmet_lod23'"/>
			
			<decal_mesh  enabled="true" name="'c_shield_glass'" />										
			<body enabled="true" name="'visor'" />													
		</sequence>
				
		<sequence editable_state="true" name="'helmet_nvg_down'" once="true" triggable="true">
			<object enabled="false" name="'g_helmet_lod9'"/>
			<object enabled="false" name="'g_helmet_lod10'"/>		
			<object enabled="true" name="'g_helmet_lod15'"/>
			<object enabled="true" name="'g_helmet_lod16'"/>
		</sequence>
		

		<sequence editable_state="true" name="'spawn_helmet'" once="true" triggable="true">
			<decal_mesh filter="'check_goggle_b'" enabled="false" name="'c_glass'" />										
			<body filter="'check_goggle_b'" enabled="false" name="'glass_swat'" />	
			<object filter="'check_goggle_b'"  enabled="false" name="'g_glass'"/>
			<object filter="'check_goggle_b'"  enabled="false" name="'g_glass_crk'"/>
			
			<decal_mesh filter="'check_goggle_d'" enabled="false" name="'c_glass'" />										
			<body filter="'check_goggle_d'" enabled="false" name="'glass_swat'" />										
			<object filter="'check_goggle_d'"  enabled="false" name="'g_glass_d'"/>
			<object filter="'check_goggle_d'"  enabled="false" name="'g_glass_d_crk'"/>
									
			<body filter="'check_city_shield'" enabled="false" name="'glass_swat'" />	
			<decal_mesh filter="'check_city_shield'" enabled="false" name="'c_glass'" />													
									
			<body filter="'check_city_shield'" enabled="false" name="'visor'" />	
			<decal_mesh filter="'check_city_shield'" enabled="false" name="'c_shield_glass'" />													

			<run_sequence filter="'check_city_shield'" name="'destroy_altyn'"/>			

			<body filter="'check_city_shield'" enabled="false" name="'shield_mask'" />	
			<decal_mesh filter="'check_city_shield'" enabled="false" name="'c_visor'" />													
									
			<body filter="'check_city_shield'" enabled="false" name="'visor'" />	
			<decal_mesh filter="'check_city_shield'" enabled="false" name="'c_shield_glass'" />													

			<effect_spawner enabled="false" name="'es_flashlight'"/>			
		</sequence>		

		<!-- BALACLAVA W/ "INSIDE" FACE SEQUENCES -->
		<sequence editable_state="true" name="'head_balaclava'" triggable="true" once="true">	
			<object enabled="true" name="'g_balaclava'"/>
			<object enabled="true" name="'g_balaclava_lod1'"/>	

			<run_sequence name="'face_'..pick('a','b','c','d','e')" />										
		</sequence>	
		
		<sequence editable_state="true" name="'head_balaclava_b'" triggable="true" once="true">	
