<sequence_manager>
	<unit>
		<variables>
			
			<!-- swat -->
			<var_gadgets_pooper value="0"/>					
			
			<!-- goggle a -->
			<var_goggles value="0"/>					
		</variables>
	
		<body name="'glass_swat'">
			<endurance bullet="1" explosion="4">
				<run_sequence name="'crk_goggles'"/>
			</endurance>
		</body>	

		<filter name="'check_ar'">
			<check value="vars.var_gadgets_pooper == 1"/>
		</filter>
		
		<filter name="'check_sg'">
			<check value="vars.var_gadgets_pooper == 2"/>
		</filter>
		
		<filter name="'check_lmg'">
			<check value="vars.var_gadgets_pooper == 3"/>
		</filter>
		
		<filter name="'check_sniper'">
			<check value="vars.var_gadgets_pooper == 4"/>
		</filter>

		<filter name="'check_goggle_a'">
			<check value="vars.var_goggles == 1"/>
		</filter>

		<filter name="'check_goggle_b'">
			<check value="vars.var_goggles == 2"/>
		</filter>
		

		<sequence editable_state="true" name="'swat_ar'" triggable="true" once="true">						
			<set_variables var_gadgets_pooper="1"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'swat_sg'" triggable="true" once="true">						
			<set_variables var_gadgets_pooper="2"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'swat_lmg'" triggable="true" once="true">						
			<set_variables var_gadgets_pooper="3"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_sniper'" triggable="true" once="true">						
			<set_variables var_gadgets_pooper="4"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'gadget_meat_grinder'" triggable="true" once="true">	
			<!-- GENERIC -->
			<run_sequence filter="'check_ar'" name="'acc'..pick('_headset','_headset','_headset', '_headset', '_headset', '_headset', '_headset', 'nothing', '_headset')"/>			
			<run_sequence filter="'check_ar'" name="'goggles_'..pick('a','a','a','a','b','a','b','a','a','a','b','c','a','a')"/>	
			<run_sequence filter="'check_ar'" name="'head_'..pick('balaclava','balaclava','balaclava', 'balaclava', 'balaclava', 'a', 'b', 'c', 'd', 'e', 'recon', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava')"/>
			<run_sequence filter="'check_ar'" name="'body_'..pick('default','default','default','default','sleeveup','default','default')"/>								
			<object filter="'check_ar'" enabled="true" name="'g_swat_ar'"/>						
			<object filter="'check_ar'" enabled="true" name="'g_swat_ar_lod1'"/>
		
			<!-- sg -->
			<object filter="'check_sg'" enabled="true" name="'g_swat_sg'"/>						
			<object filter="'check_sg'" enabled="true" name="'g_swat_sg_lod1'"/>
			<run_sequence filter="'check_sg'" name="'acc'..pick('_headset','_headset','_headset', '_headset', '_headset', '_headset', '_headset', 'nothing', '_headset')"/>						
			<run_sequence filter="'check_sg'" name="'head_'..pick('a','b','c', 'd', 'a', 'balaclava', 'c', 'd', 'e')"/>			
			<run_sequence filter="'check_sg'" name="'body_'..pick('sleeveup','sleeveup','sleeveup','sleeveup','default','sleeveup','sleeveup')"/>									
			<run_sequence filter="'check_sg'" name="'goggles_'..pick('c','c','c', 'c', 'nothing', 'c')"/>				
			<object filter="'check_sg'" enabled="true" name="'g_hat_lod1'"/>						
			<object filter="'check_sg'" enabled="true" name="'g_hat_lod2'"/>
			<object filter="'check_sg'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_sg'" enabled="false" name="'g_helmet_lod2'"/>			
			<decal_mesh filter="'check_sg'" enabled="false" name="'dm_helmet'"/>					
		
			<!-- lmg -->
			<run_sequence filter="'check_lmg'" name="'body_lmg'"/>									
			<run_sequence filter="'check_lmg'" name="'head_'..pick('balaclava_b','balaclava_b','balaclava_b', 'balaclava_b', 'balaclava_b', 'a', 'b', 'c', 'd', 'e', 'balaclava_b', 'balaclava_b', 'balaclava_b', 'balaclava_b')"/>
			<run_sequence filter="'check_lmg'" name="'goggles_'..pick('nothing','nothing','nothing', 'c', 'a', 'b', 'nothing', 'nothing', 'nothing')"/>	
			<object filter="'check_lmg'" enabled="true" name="'g_helmet_lod3'"/>						
			<object filter="'check_lmg'" enabled="true" name="'g_helmet_lod4'"/>			
			<run_sequence filter="'check_lmg'" name="'acc'..pick('_headset','_headset','_headset', '_headset', '_headset', '_headset', '_headset', 'nothing', '_headset')"/>			
		
			<!-- dmr -->
			<run_sequence filter="'check_sniper'" name="'head_'..pick('recon', 'recon','a', 'balaclava', 'c', 'a', 'b', 'c', 'd', 'e')"/>			
			<run_sequence filter="'check_sniper'" name="'body_black_'..pick('default','default','default','default','sleeveup','default','default')"/>								
			<run_sequence filter="'check_sniper'" name="'goggles_'..pick('nothing','nothing','nothing', 'c', 'nothing', 'nothing', 'nothing', 'nothing', 'nothing')"/>	
			<object filter="'check_sniper'" enabled="true" name="'g_hat_lod3'"/>						
			<object filter="'check_sniper'" enabled="true" name="'g_hat_lod4'"/>
			<object filter="'check_sniper'" enabled="true" name="'g_sniper'"/>						
			<object filter="'check_sniper'" enabled="true" name="'g_sniper_lod1'"/>				
			<object filter="'check_sniper'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_sniper'" enabled="false" name="'g_helmet_lod2'"/>			
			<decal_mesh filter="'check_sniper'" enabled="false" name="'dm_helmet'"/>							
		</sequence>
		
		<sequence editable_state="true" name="'acc_headset'" once="true" triggable="true">				
			<object enabled="true" name="'g_headset'"/>
			<object enabled="true" name="'g_headset_lod1'"/>				
		</sequence>		

		<sequence editable_state="true" name="'spawn_helmet'" once="true" triggable="true">
			<decal_mesh filter="'check_goggle_b'" enabled="false" name="'c_glass'" />										
			<object filter="'check_goggle_b'" enabled="false" name="'g_goggles_a_lod1'" />										
			<object filter="'check_goggle_b'" enabled="false" name="'g_glass'" />										
			<object filter="'check_goggle_b'" enabled="false" name="'g_glass_crk'" />																			
		</sequence>

		
		<sequence editable_state="true" name="'goggles_a'" once="true" triggable="true">
			<object enabled="true" name="'g_goggles_a'"/>
			<object enabled="true" name="'g_goggles_a_lod1'"/>
			<object enabled="true" name="'g_glass'"/>
			<set_variables var_goggles="1"/>					
			<decal_mesh enabled="true" name="'c_glass'" />					
			<body enabled="true" name="'glass_swat'" />					
		</sequence>
		
		<sequence editable_state="true" name="'goggles_b'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod5'"/>
			<object enabled="true" name="'g_glass'"/>
			<set_variables var_goggles="2"/>										
			<decal_mesh enabled="true" name="'c_glass'" />					
			<body enabled="true" name="'glass_swat'" />										
		</sequence>

		<sequence editable_state="true" name="'crk_goggles'" once="true" triggable="true">
			<object enabled="true" name="'g_glass_crk'"/>
			<object enabled="false" name="'g_glass'"/>
		</sequence>				
		

		<sequence editable_state="true" name="'goggles_c'" once="true" triggable="true">
			<object enabled="true" name="'g_glasses'"/>
			<object enabled="true" name="'g_glasses_glass'"/>
		</sequence>
		<!-- RANDOM HELMETS end -->
						
		<!-- RANDOM HOLSTERS -->
		<sequence editable_state="true" name="'body_default'" once="true" triggable="true">
			<object enabled="true" name="'g_body'"/>
			<object enabled="true" name="'g_body_lod1'"/>
			
			<object enabled="false" name="'g_arms_a'"/>
			<object enabled="false" name="'g_arms_b'"/>
			<object enabled="false" name="'g_arms_c'"/>
			<object enabled="false" name="'g_arms_d'"/>
