<sequence_manager>
	<unit>
		<variables>
										
			<!-- heavy_fbi swat -->
			<var_gadgets_fbi_heavy value="0"/>	
			
			<var_gadgets_flashlight value="0"/>					
			
			<!-- fbi swat -->
			<var_gadgets_fbi_light value="0"/>					
			
			<!-- goggle a -->
			<var_goggles value="0"/>					
		</variables>
	

		<body name="'glass_swat'">
			<endurance explosion="4" bullet="1">
				<run_sequence name="'crk_goggles'"/>
				<run_sequence name="'crk_visor'"/>
			</endurance>
		</body>	
		
		<body name="'glass_altyn'">
			<endurance explosion="5" bullet="1">
				<run_sequence name="'crk_visor'"/>
			</endurance>
		</body>	
		
		<body name="'visor'">
			<endurance explosion="6" >
				<run_sequence name="'destroy_altyn'"/>
			</endurance>
		</body>	
		
		<body name="'glass_c'">
			<endurance bullet="1">
				<run_sequence name="'crk_goggles'"/>
			</endurance>
		</body>	

		<filter name="'check_fbi_swat_ar'">
			<check value="vars.var_gadgets_fbi_light == 1"/>
		</filter>		
		<filter name="'check_fbi_swat_sg'">
			<check value="vars.var_gadgets_fbi_light == 2"/>
		</filter>
		<filter name="'check_fbi_swat_smg'">
			<check value="vars.var_gadgets_fbi_light == 3"/>
		</filter>
		<filter name="'check_fbi_shield'">
			<check value="vars.var_gadgets_fbi_light == 4"/>
		</filter>
		<filter name="'check_fbi_sniper'">
			<check value="vars.var_gadgets_fbi_light == 5"/>
		</filter>

		<filter name="'check_goggle_a'">
			<check value="vars.var_goggles == 1"/>
		</filter>

		<filter name="'check_flashlights_on_sg'">
			<check value="vars.var_gadgets_flashlight == 1"/>
			<check value="vars.var_gadgets_fbi_light == 2"/>
		</filter>

		<filter name="'check_flashlights_on_ar'">
			<check value="vars.var_gadgets_flashlight == 1"/>
			<check value="vars.var_gadgets_fbi_light == 1"/>
		</filter>

		<filter name="'check_goggle_b'">
			<check value="vars.var_goggles == 2"/>
		</filter>
		
		<filter name="'check_goggle_c'">
			<check value="vars.var_goggles == 3"/>
		</filter>
		
		<filter name="'check_goggle_d'">
			<check value="vars.var_goggles == 4"/>
		</filter>
		
		
		<sequence editable_state="true" name="'swat_ar'" triggable="true" once="true">						
			<set_variables var_gadgets_fbi_light="1"/>			
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_ar_helmetlight'" triggable="true" once="true">						
			<set_variables var_gadgets_fbi_light="1"/>
			<set_variables var_gadgets_flashlight="1"/>			
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_sg'" triggable="true" once="true">						
			<set_variables var_gadgets_fbi_light="2"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'swat_sg_nvg'" triggable="true" once="true">	
			<set_variables var_gadgets_fbi_light="2"/>		
			<set_variables var_gadgets_flashlight="1"/>
			<run_sequence name="'gadget_meat_grinder'"/>					
		</sequence>

		<sequence editable_state="true" name="'swat_smg'" triggable="true" once="true">						
			<set_variables var_gadgets_fbi_light="3"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>
		
		<sequence editable_state="true" name="'swat_shield'" triggable="true" once="true">						
			<set_variables var_gadgets_fbi_light="4"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'swat_sniper'" triggable="true" once="true">						
			<set_variables var_gadgets_fbi_light="5"/>
			<run_sequence name="'gadget_meat_grinder'"/>		
		</sequence>

		<sequence editable_state="true" name="'gadget_meat_grinder'" triggable="true" once="true">	
			
			<!-- rifleman -->
			<run_sequence filter="'check_fbi_swat_ar'" name="'goggles_'..pick('a','a','a','a','a','b','b','d','a','a')"/>								
			<run_sequence filter="'check_fbi_swat_ar'" name="'head_'..pick('balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'a','b','c', 'balaclava', 'balaclava')"/>
			<object filter="'check_fbi_swat_ar'" enabled="true" name="'g_swat_ar'"/>						
			<object filter="'check_fbi_swat_ar'" enabled="true" name="'g_swat_ar_lod1'"/>
			<run_sequence filter="'check_fbi_swat_ar'" name="'holster_'..pick('r','r','r','r','l','l','twinnem')"/>
			<run_sequence filter="'check_fbi_swat_ar'" name="'helmet'..pick('_flashlight','_flashlight','_flashlight','_flashlight','_flashlight','nun','_flashlight')"/>

			<!-- ar (flashlight on)-->
			<run_sequence filter="'check_flashlights_on_ar'" name="'flashlight_on'"/>
			<run_sequence filter="'check_flashlights_on_ar'" name="'helmet_flashlight'"/>
		
			<!-- sg -->
			<run_sequence filter="'check_fbi_swat_sg'" name="'goggles_'..pick('a','a','a','a','a','b','b','d','a','a')"/>								
			<run_sequence filter="'check_fbi_swat_sg'" name="'head_'..pick('balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'a','b','c', 'balaclava', 'balaclava')"/>
			<object filter="'check_fbi_swat_sg'" enabled="true" name="'g_swat_sg'"/>						
			<object filter="'check_fbi_swat_sg'" enabled="true" name="'g_swat_sg_lod1'"/>
			<object filter="'check_fbi_swat_sg'" enabled="true" name="'g_body_shitbag'"/>						
			<object filter="'check_fbi_swat_sg'" enabled="true" name="'g_body_shitbag_lod1'"/>
			<object filter="'check_fbi_swat_sg'" enabled="false" name="'g_body'"/>						
			<object filter="'check_fbi_swat_sg'" enabled="false" name="'g_body_lod1'"/>
			<run_sequence filter="'check_fbi_swat_sg'" name="'holster_'..pick('r','r','r','r','l','l','twinnem')"/>
			<run_sequence filter="'check_fbi_swat_sg'" name="'helmet_nvg_up'"/>
		
			<!-- sg (nvgs on)-->
			<run_sequence filter="'check_flashlights_on_sg'" name="'helmet_nvg_down'"/>

			<!-- smg -->			
			<run_sequence filter="'check_fbi_swat_smg'" name="'goggles_c'"/>								
			<run_sequence filter="'check_fbi_swat_smg'" name="'head_'..pick('balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'balaclava', 'a','b','c', 'balaclava', 'balaclava')"/>
			<object filter="'check_fbi_swat_smg'" enabled="true" name="'g_body_shitbag'"/>						
			<object filter="'check_fbi_swat_smg'" enabled="true" name="'g_body_shitbag_lod1'"/>
			<object filter="'check_fbi_swat_smg'" enabled="false" name="'g_body'"/>						
			<object filter="'check_fbi_swat_smg'" enabled="false" name="'g_body_lod1'"/>
			<object filter="'check_fbi_swat_smg'" enabled="true" name="'g_swat_smg'"/>						
			<object filter="'check_fbi_swat_smg'" enabled="true" name="'g_swat_smg_lod1'"/>
			<object filter="'check_fbi_swat_smg'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_fbi_swat_smg'" enabled="false" name="'g_helmet_lod2'"/>
			<run_sequence filter="'check_fbi_swat_smg'" name="'holster_'..pick('r','r','r','r','l','l','twinnem')"/>

			<!-- shield -->			
			<object filter="'check_fbi_shield'" enabled="true" name="'g_shield'"/>						
			<object filter="'check_fbi_shield'" enabled="true" name="'g_shield_lod1'"/>
			<object filter="'check_fbi_shield'" enabled="true" name="'g_body_shitbag'"/>						
			<object filter="'check_fbi_shield'" enabled="true" name="'g_body_shitbag_lod1'"/>
			<object filter="'check_fbi_shield'" enabled="false" name="'g_body'"/>						
			<object filter="'check_fbi_shield'" enabled="false" name="'g_body_lod1'"/>			
			<run_sequence filter="'check_fbi_shield'" name="'head_balaclava'"/>
			<run_sequence filter="'check_fbi_shield'" name="'helmet_altyn'"/>
			<run_sequence filter="'check_fbi_shield'" name="'holster_'..pick('r_shield','l_shield')"/>
			
						
			<!-- sniper -->			
			<object filter="'check_fbi_sniper'" enabled="true" name="'g_sniper'"/>						
			<object filter="'check_fbi_sniper'" enabled="true" name="'g_sniper_lod1'"/>
			<object filter="'check_fbi_sniper'" enabled="false" name="'g_helmet_lod1'"/>						
			<object filter="'check_fbi_sniper'" enabled="false" name="'g_helmet_lod2'"/>
			<object filter="'check_fbi_sniper'" enabled="true" name="'g_hat_lod1'"/>
			<object filter="'check_fbi_sniper'" enabled="true" name="'g_hat_lod2'"/>					
			<decal_mesh filter="'check_fbi_sniper'" enabled="false" name="'dm_helmet'"/>					
			<run_sequence filter="'check_fbi_sniper'" name="'head_'..pick('a','b','c', 'balaclava')"/>

			<run_sequence filter="'check_fbi_sniper'" name="'goggles_'..pick('a','a','a','a','a','d','nothing','nothing')"/>													
		</sequence>

		<sequence editable_state="true" name="'crk_visor'" once="true" triggable="true">
			<object filter="'check_fbi_shield'" enabled="false" name="'g_helmet_lod13'"/>
			<object filter="'check_fbi_shield'" enabled="true" name="'g_helmet_lod14'"/>
			<effect filter="'check_fbi_shield'" damp_time="5" name="'effects/particles/bullet_hit/glass_breakable/bullet_hit_glass_breakable'" parent="object( 'Head' )" position="v()"/>
		</sequence>				

		<sequence editable_state="true" name="'destroy_altyn'" once="true" triggable="true">
			<object filter="'check_fbi_shield'" enabled="false" name="'g_helmet_lod11'"/>
			<object filter="'check_fbi_shield'" enabled="false" name="'g_helmet_lod12'"/>
			<object filter="'check_fbi_shield'" enabled="false" name="'g_helmet_lod13'"/>
			<object filter="'check_fbi_shield'" enabled="false" name="'g_helmet_lod14'"/>
			<body 	filter="'check_fbi_shield'" enabled="false" name="'visor'" />
			<body 	filter="'check_fbi_shield'" enabled="false" name="'glass_altyn'" />
			<decal_mesh filter="'check_fbi_shield'" enabled="false" name="'c_altyn_glass'" />
			<decal_mesh filter="'check_fbi_shield'" enabled="false" name="'c_altyn_visor'" />
			<effect filter="'check_fbi_shield'" damp_time="4" name="'effects/payday2/particles/impacts/metal_impact_pd2'" parent="object( 'Head' )" position="v()"/>			
			<effect filter="'check_fbi_shield'" damp_time="5" name="'effects/payday2/particles/impacts/metal_impact_pd2'" parent="object( 'Head' )" position="v()"/>
			
			<sound action="'play'" event="'swatturret_weakspot_hit'" object="'Head'"/>					
		</sequence>				

		<sequence editable_state="true" name="'goggles_a'" once="true" triggable="true">
			<object enabled="true" name="'g_goggles_a'"/>
			<object enabled="true" name="'g_goggles_a_lod1'"/>
			<object enabled="true" name="'g_glass'"/>
			<set_variables var_goggles="1"/>					
			<decal_mesh enabled="true" name="'dm_glass'" />					
			<body enabled="true" name="'glass_swat'" />					
		</sequence>

		<sequence editable_state="true" name="'crk_goggles'" once="true" triggable="true">
			<object filter="'check_goggle_a'" enabled="true" name="'g_glass_crk'"/>
			<object filter="'check_goggle_a'" enabled="false" name="'g_glass'"/>
			
			<object filter="'check_goggle_b'" enabled="true" name="'g_helmet_lod6'"/>
			<object filter="'check_goggle_b'" enabled="false" name="'g_helmet_lod5'"/>	
			
			<object filter="'check_goggle_c'" enabled="false" name="'g_goggles_c_glass'"/>
			<object filter="'check_goggle_c'" enabled="true" name="'g_goggles_c_glass_crk'"/>		
		</sequence>				
		
		<sequence editable_state="true" name="'goggles_b'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod3'"/>
			<object enabled="true" name="'g_helmet_lod4'"/>
			<object enabled="true" name="'g_helmet_lod5'"/>
			<set_variables var_goggles="2"/>										
			<decal_mesh enabled="true" name="'dm_glass'" />					
			<body enabled="true" name="'glass_swat'" />										
		</sequence>
		
		<sequence editable_state="true" name="'goggles_c'" once="true" triggable="true">
			<object enabled="true" name="'g_goggles_c'"/>
			<object enabled="true" name="'g_goggles_c_lod1'"/>
			<object enabled="true" name="'g_goggles_c_glass'"/>
			<set_variables var_goggles="3"/>										
			<decal_mesh enabled="true" name="'c_glass_c'" />					
			<body enabled="true" name="'glass_c'" />										
		</sequence>
		
		<sequence editable_state="true" name="'goggles_d'" once="true" triggable="true">
			<object enabled="true" name="'g_glasses'"/>
			<object enabled="true" name="'g_glasses_glass'"/>
			<set_variables var_goggles="4"/>										
		</sequence>

		<sequence editable_state="true" name="'helmet_flashlight'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod7'"/>
			<object enabled="true" name="'g_helmet_lod8'"/>
		</sequence>

		<sequence editable_state="true" name="'flashlight_on'" once="true" triggable="true">
			<effect_spawner enabled="true" name="'es_flashlight'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_nvg_up'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod9'"/>
			<object enabled="true" name="'g_helmet_lod10'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_nvg_down'" once="true" triggable="true">
			<object enabled="false" name="'g_helmet_lod9'"/>
			<object enabled="false" name="'g_helmet_lod10'"/>		
			<object enabled="true" name="'g_helmet_lod15'"/>
			<object enabled="true" name="'g_helmet_lod16'"/>
		</sequence>
		
		<sequence editable_state="true" name="'helmet_altyn'" once="true" triggable="true">
			<object enabled="true" name="'g_helmet_lod11'"/>
			<object enabled="true" name="'g_helmet_lod12'"/>
			
			<object enabled="true" name="'g_helmet_lod13'"/>

			<decal_mesh enabled="true" name="'c_altyn_glass'" />					
			<decal_mesh enabled="true" name="'c_altyn_visor'" />					
			<body enabled="true" name="'glass_altyn'" />					
			<body enabled="true" name="'visor'" />					
		</sequence>

		<sequence editable_state="true" name="'spawn_helmet'" once="true" triggable="true">
			<decal_mesh filter="'check_goggle_b'" enabled="false" name="'c_glass'" />										
			<body filter="'check_goggle_b'" enabled="false" name="'glass_swat'" />										
									
			<body filter="'check_fbi_shield'" enabled="false" name="'glass_swat'" />	
			<decal_mesh filter="'check_fbi_shield'" enabled="false" name="'c_glass'" />													
									
			<body filter="'check_fbi_shield'" enabled="false" name="'visor'" />	
			<decal_mesh filter="'check_fbi_shield'" enabled="false" name="'c_altyn_visor'" />													
									
			<body filter="'check_fbi_shield'" enabled="false" name="'glass_altyn'" />	
			<decal_mesh filter="'check_fbi_shield'" enabled="false" name="'c_altyn_glass'" />	


			<run_sequence filter="'check_fbi_shield'" name="'destroy_altyn'"/>			

			<effect_spawner enabled="false" name="'es_flashlight'"/>			
		</sequence>		

		<!-- BALACLAVA W/ "INSIDE" FACE SEQUENCES -->
		<sequence editable_state="true" name="'head_balaclava'" triggable="true" once="true">	
			<object enabled="true" name="'g_balaclava'"/>
			<object enabled="true" name="'g_balaclava_lod1'"/>	

			<run_sequence name="'face_'..pick('a','b','c','d','e')" />										
		</sequence>								

		<!-- RANDOM HOLSTERS -->
		<sequence editable_state="true" name="'holster_l'" once="true" triggable="true">
			<object enabled="true" name="'g_holster_l'"/>
			<object enabled="true" name="'g_holster_l_lod1'"/>
			<object enabled="false" name="'g_holster_r'"/>
			<object enabled="false" name="'g_holster_r_lod1'"/>
		</sequence>

		<sequence editable_state="true" name="'holster_r'" once="true" triggable="true">
			<object enabled="true" name="'g_holster_l'"/>
			<object enabled="true" name="'g_holster_l_lod1'"/>
		</sequence>
		
		<sequence editable_state="true" name="'holster_l_shield'" once="true" triggable="true">
			<object enabled="true" name="'g_holster_l_shield'"/>
			<object enabled="true" name="'g_holster_l_shield_lod1'"/>
		</sequence>

		<sequence editable_state="true" name="'holster_r_shield'" once="true" triggable="true">
			<object enabled="true" name="'g_holster_r_shield'"/>
			<object enabled="true" name="'g_holster_r_shield_lod1'"/>
		</sequence>																

		<sequence editable_state="true" name="'holster_twinnem'" once="true" triggable="true">
			<object enabled="true" name="'g_holster_l'"/>
			<object enabled="true" name="'g_holster_l_lod1'"/>
			<object enabled="true" name="'g_holster_r'"/>
			<object enabled="true" name="'g_holster_r_lod1'"/>
		</sequence>								
		<!-- RANDOM HOLSTERS end -->
		
		<sequence editable_state="true" name="'face_a'" once="true" triggable="true">
			<object enabled="true" name="'g_face_a'"/>
			<object enabled="true" name="'g_face_a_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'face_b'" once="true" triggable="true">
			<object enabled="true" name="'g_face_b'"/>
			<object enabled="true" name="'g_face_b_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'face_c'" once="true" triggable="true">
			<object enabled="true" name="'g_face_c'"/>
			<object enabled="true" name="'g_face_c_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'face_d'" once="true" triggable="true">
			<object enabled="true" name="'g_face_d'"/>
			<object enabled="true" name="'g_face_d_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'face_e'" once="true" triggable="true">
			<object enabled="true" name="'g_face_e'"/>
			<object enabled="true" name="'g_face_e_lod1'"/>
		</sequence>	
		<!-- END BALACALVA FACES -->

		<!-- EXPOSED HEAD SEQUENCES -->				
		<sequence editable_state="true" name="'head_a'" once="true" triggable="true">
			<object enabled="true" name="'g_head_a'"/>
			<object enabled="true" name="'g_head_a_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'head_b'" once="true" triggable="true">
			<object enabled="true" name="'g_head_b'"/>
			<object enabled="true" name="'g_head_b_lod1'"/>
		</sequence>								
		<sequence editable_state="true" name="'head_c'" once="true" triggable="true">
			<object enabled="true" name="'g_head_c'"/>
			<object enabled="true" name="'g_head_c_lod1'"/>
		</sequence>	
	
		<sequence editable_state="true" name="'disable_hitbox'" triggable="true">
			<body enabled="false" name="'body'"/>
			<body enabled="false" name="'Head'"/>
			<body enabled="false" name="'rag_LeftLeg'"/>
			<body enabled="false" name="'rag_RightLeg'"/>
			<body enabled="false" name="'rag_RightUpLeg'"/>
			<body enabled="false" name="'rag_LeftUpLeg'"/>
		</sequence>
		<sequence editable_state="true" name="'leg_arm_hitbox'" triggable="true">
			<body enabled="true" name="'body'"/>
			<body enabled="true" name="'Head'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Hips'"/>
			<body enabled="true" motion="'keyframed'" name="'rag_LeftUpLeg'"/>
			<body enabled="true" motion="'keyframed'" name="'rag_RightUpLeg'"/>
			<body enabled="true" motion="'keyframed'" name="'rag_LeftLeg'"/>
			<body enabled="true" motion="'keyframed'" name="'rag_RightLeg'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Spine'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Spine1'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Spine2'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_LeftForeArm'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_RightForeArm'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_LeftArm'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_RightArm'"/>
			<body enabled="false" motion="'keyframed'" name="'rag_Head'"/>
			<constraint enabled="false" name="'LeftUpLeg'"/>
			<constraint enabled="false" name="'RightUpLeg'"/>
			<constraint enabled="false" name="'LeftLeg'"/>
			<constraint enabled="false" name="'RightLeg'"/>
			<constraint enabled="false" name="'Spine'"/>
			<constraint enabled="false" name="'Spine1'"/>
			<constraint enabled="false" name="'Spine2'"/>
			<constraint enabled="false" name="'LeftForeArm'"/>
			<constraint enabled="false" name="'RightForeArm'"/>
			<constraint enabled="false" name="'LeftArm'"/>
			<constraint enabled="false" name="'RightArm'"/>
			<constraint enabled="false" name="'Head'"/>
		</sequence>
		<sequence editable_state="true" name="'prepare_ragdoll'" triggable="true">
			<body enabled="false" motion="'dynamic'" name="'rag_Hips'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_LeftUpLeg'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_RightUpLeg'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_LeftLeg'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_RightLeg'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_Spine'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_Spine1'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_Spine2'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_LeftForeArm'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_RightForeArm'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_LeftArm'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_RightArm'"/>
			<body enabled="false" motion="'dynamic'" name="'rag_Head'"/>
		</sequence>
		<sequence editable_state="true" name="'switch_to_ragdoll'" triggable="true">
			<body enabled="false" name="'body'"/>
			<body enabled="false" name="'Head'"/>
